<style>
  #loading-screen {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    background: linear-gradient(145deg, #0d0d0d, #1a1a1a 20%, #4f4f4f 50%, #1a1a1a 80%, #000000);
    z-index: 9999;
  }

  #loading-screen-logo {
    animation: spin 2.8s infinite;
    margin-bottom: 6.25rem;
  }

  #loading-screen-alphabet {
    display: flex;
    gap: 0.5rem;
    margin-block-start: 0;
    margin-block-end: 0;
    padding-inline-start: 0;
  }

  #loading-screen-alphabet > li {
    font-family: monospace;
    list-style: none;
    font-size: 1.5rem;
    line-height: 1;
    color: #b7ec5d;
    animation: beat 0.5s calc(var(--i) * 0.16s) alternate infinite;
  }

  #loading-progress {
    margin-top: 1.5rem;
    width: 25rem;
    height: 0.3rem;
    appearance: none;
    max-width: calc(100% - 3rem);
  }

  @media (min-width: 768px) {
    #loading-progress {
      margin-top: 2rem;
      height: 0.4rem;
    }
  }

  #loading-progress::-webkit-progress-bar {
    background-color: #666666;
    border-radius: 0.25rem;
  }

  #loading-progress::-webkit-progress-value {
    background-color: #b7ec5d;
    border-radius: 0.25rem;
    transition: width 300ms cubic-bezier(0.4, 0, 0.2, 1);
  }

  #loading-reload {
    display: none;
    position: absolute;
    bottom: 10%;
    width: 100%;
    justify-content: center;
  }

  @keyframes spin {
    0% {
      transform: rotate3d(0, 1, 0, 0deg) scale(1.1);
    }
    50% {
      transform: rotate3d(0, 1, 0, 360deg) scale(0.75);
    }
    100% {
      transform: rotate3d(0, 1, 0, 720deg) scale(1.1);
    }
  }

  @keyframes beat {
    from {
      transform: scale(1.25);
    }
    to {
      transform: translateY(-5rem) scale(1);
    }
  }
</style>

<div id="loading-screen">
  <img
    id="loading-screen-logo"
    src="data:image/png;base64,"
    width="150"
    alt="PDF Signature Logo"
  />

  <ul id="loading-screen-alphabet">
    <li style="--i: 0">P</li>
    <li style="--i: 1">D</li>
    <li style="--i: 2">F</li>
    <li style="--i: 3">-</li>
    <li style="--i: 4">S</li>
    <li style="--i: 5">I</li>
    <li style="--i: 6">G</li>
    <li style="--i: 7">N</li>
    <li style="--i: 8">A</li>
    <li style="--i: 9">T</li>
    <li style="--i: 10">U</li>
    <li style="--i: 11">R</li>
    <li style="--i: 12">E</li>
  </ul>

  <progress
    id="loading-progress"
    value="0"
    max="100"
  ></progress>

  <div id="loading-reload"></div>
</div>

<script>
  const progressBar = document.getElementById('loading-progress');

  const interval = setInterval(() => {
    const value = parseInt(progressBar.value);

    if (value >= 40) {
      clearInterval(interval);
    } else {
      progressBar.value += 1;
    }
  }, 50);
</script>
